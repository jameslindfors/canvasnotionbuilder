{
   "name": "canvasnotionbuilder",
   "version": "1.0.0",
   "description": "API that utilizes Canvas Learning System API and Notion API to manage and track Canvas data in Notion pages.",
   "main": "./dist/index.js",
   "scripts": {
      "dev": "concurrently \"tsc -w\" \"nodemon ./dist/index.js\"",
      "run:node": "nodemon ./dist/index.js",
      "watch:ts": "tsc -w",
      "build:ts": "tsc",
      "start": "ts-node src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "keywords": [],
   "author": "James Lindfors",
   "license": "BSD-3-Clause",
   "devDependencies": {
      "@types/koa": "^2.13.4",
      "@types/koa__router": "^8.0.11",
      "@types/node": "^16.11.10",
      "concurrently": "^7.0.0",
      "nodemon": "^2.0.15",
      "typescript": "4.5.2",
      "ts-node": "10.4.0"
   },
   "dependencies": {
      "@koa/router": "^10.1.1",
      "@notionhq/client": "^1.0.3",
      "axios": "^0.26.1",
      "koa": "^2.13.4",
      "pg": "^8.4.0",
      "ts-dotenv": "^0.8.3",
      "typeorm": "0.2.45",
      "reflect-metadata": "^0.1.13"
   }
}